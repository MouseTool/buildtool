{
  "name": "buildtool",
  "version": "2.5.0",
  "description": "Buildtowol",
  "main": "dummy.js",
  "scripts": {
    "build": "node build-translations.js && onelua . --no-minify --prepend-meta",
    "build:watch": "onchange \"src/**/*.lua\" -i -- onelua . --no-minify --prepend-meta",
    "minify": "node build-translations.js && onelua . -o buildtool-minified.lua",
    "regentl": "py regen-translations.py",
    "buildtl": "node build-translations.js"
  },
  "onelua": {
    "main": "src/main.lua",
    "output": "buildtool.lua"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mousetool/linkedlist": "^1.0.5",
    "@mousetool/mousebase": "1.1.0",
    "@mousetool/mousexml": "^1.0.3",
    "@mousetool/ordered-table": "^1.0.1"
  },
  "devDependencies": {
    "@mousetool/onelua": "^1.1.1",
    "@mousetool/tfm-vscode": "^2.0.0",
    "onchange": "^7.1.0"
  }
}
